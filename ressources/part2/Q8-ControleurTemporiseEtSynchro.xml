<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
chan r1,r2,cv1,cv2,o,r;</declaration>
	<template>
		<name x="5" y="5">P0</name>
		<declaration>// Place local declarations here.
clock t;</declaration>
		<location id="id0" x="-102" y="-34">
		</location>
		<location id="id1" x="-136" y="0">
			<name x="-146" y="-34">O1</name>
		</location>
		<location id="id2" x="-68" y="-68">
			<name x="-78" y="-102">R1</name>
		</location>
		<location id="id3" x="0" y="0">
			<name x="-10" y="-34">V1</name>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-126" y="-85">r1!</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-140" y="-51">r?</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-118" y="-17">o?</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-34" y="-59">cv1?</label>
		</transition>
	</template>
	<template>
		<name>P1</name>
		<declaration>clock s;</declaration>
		<location id="id4" x="-34" y="34">
		</location>
		<location id="id5" x="-68" y="68">
			<name x="-78" y="34">O2</name>
		</location>
		<location id="id6" x="68" y="68">
			<name x="58" y="34">V2</name>
		</location>
		<location id="id7" x="0" y="0">
			<name x="-10" y="-34">R2</name>
		</location>
		<init ref="id7"/>
		<transition>
			<source ref="id4"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-50" y="-17">r2!</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="-68" y="17">r?</label>
		</transition>
		<transition>
			<source ref="id6"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="-17" y="42">o?</label>
		</transition>
		<transition>
			<source ref="id7"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="34" y="17">cv2?</label>
		</transition>
	</template>
	<template>
		<name>C</name>
		<declaration>clock t;</declaration>
		<location id="id8" x="34" y="-68">
			<name x="42" y="-102">V1</name>
			<label kind="invariant" x="24" y="-51">t&lt;=0</label>
		</location>
		<location id="id9" x="102" y="-102">
			<name x="92" y="-136">V2</name>
			<label kind="invariant" x="92" y="-85">t&lt;=0</label>
		</location>
		<location id="id10" x="0" y="-170">
			<name x="-10" y="-204">TR</name>
			<label kind="invariant" x="-17" y="-229">t&lt;=9</label>
		</location>
		<location id="id11" x="-170" y="0">
			<name x="-180" y="-34">TO</name>
			<label kind="invariant" x="-180" y="17">t&lt;=2</label>
		</location>
		<location id="id12" x="136" y="0">
			<name x="126" y="-34">TV</name>
			<label kind="invariant" x="126" y="17">t&lt;=5</label>
		</location>
		<init ref="id12"/>
		<transition>
			<source ref="id9"/>
			<target ref="id12"/>
			<label kind="synchronisation" x="119" y="-68">cv2!</label>
			<label kind="assignment" x="119" y="-51">t:=0</label>
		</transition>
		<transition>
			<source ref="id8"/>
			<target ref="id12"/>
			<label kind="synchronisation" x="51" y="-42">cv1!</label>
			<label kind="assignment" x="52" y="-34">t:=0</label>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id8"/>
			<label kind="guard" x="-25" y="-119">t==9</label>
			<label kind="synchronisation" x="17" y="-136">r2?</label>
			<label kind="assignment" x="-25" y="-102">t:=0</label>
		</transition>
		<transition>
			<source ref="id10"/>
			<target ref="id9"/>
			<label kind="guard" x="68" y="-178">t==9</label>
			<label kind="synchronisation" x="51" y="-161">r1?</label>
			<label kind="assignment" x="76" y="-161">t:=0</label>
		</transition>
		<transition>
			<source ref="id11"/>
			<target ref="id10"/>
			<label kind="guard" x="-127" y="-102">t==2</label>
			<label kind="synchronisation" x="-152" y="-102">r!</label>
			<label kind="assignment" x="-152" y="-85">t:=0</label>
		</transition>
		<transition>
			<source ref="id12"/>
			<target ref="id11"/>
			<label kind="guard" x="-34" y="-17">t==5</label>
			<label kind="synchronisation" x="-59" y="-34">o!</label>
			<label kind="assignment" x="-25" y="0">t:=0</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
feu1 = P0();
feu2 = P1();
controleur = C();
// List one or more processes to be composed into a system.
system feu1,feu2,controleur;
    </system>
	<queries>
		<query>
			<formula>E&lt;&gt;deadlock
			</formula>
			<comment>
			</comment>
		</query>
		<query>
			<formula>A[] (feu1.R1 or feu2.R2)
			</formula>
			<comment>
			</comment>
		</query>
	</queries>
</nta>
